                    <script>
                        const main = document.getElementById("main_section");
                        const zerotoh = document.getElementById("zerotoh");
                        const onboarding = document.getElementById("onboarding");
                        const zerotoo = document.getElementById("zerotoo");
                        const consume_back = document.getElementById("consume_back");
                        const consume = document.getElementById("course_consume");
                        const vid_data = document.getElementById("video_data");
                        const next_vid = document.getElementById("next_vid");
                        function openDetail(course_id) {
                        if (course_id == '0-100'){
                            main.classList.add("d-none");
                            zerotoh.classList.remove("d-none");
                        }
                        if (course_id == 'onboarding'){
                            main.classList.add("d-none");
                            onboarding.classList.remove("d-none");
                        }
                        if (course_id == 'zerotoo'){
                            main.classList.add("d-none");
                            zerotoo.classList.remove("d-none");
                        }
                        }
                        function backMain(type) {
                        if (type == '0-100'){
                            zerotoh.classList.add("d-none");
                            main.classList.remove("d-none");
                        }
                        if (type == 'onboarding'){
                            onboarding.classList.add("d-none");
                            main.classList.remove("d-none");
                        }
                        if (type == 'zerotoo'){
                            zerotoo.classList.add("d-none");
                            main.classList.remove("d-none");
                        }
                        }
                        function backDetail(type) {
                        let vid_data_inner = vid_data.innerHTML;
                        if (type == '0-100'){
                            consume.classList.add("d-none");
                            zerotoh.classList.remove("d-none");
                            vid_data.innerHTML = vid_data_inner;
                        }
                        if (type == 'onboarding'){
                            consume.classList.add("d-none");
                            onboarding.classList.remove("d-none")
                            vid_data.innerHTML = vid_data_inner;
                        }
                        if (type == 'zerotoo'){
                            consume.classList.add("d-none");
                            zerotoo.classList.remove("d-none")
                            vid_data.innerHTML = vid_data_inner;
                        }
                        }
                        function openVideo(course_no, type) {
                        fetch('/partner/training/video?video_no=' + course_no + '&amp;type=' + type, {
                        method: "POST",
                        headers: {
                        "Content-Type": "application/json",
                        },
                        'body': JSON.stringify({'params': {'video_no': course_no}})
                        })
                        .then(response =&gt; response.json())
                        .then(data =&gt; {
                        if (data['response'] == 'success') {
                        if (data['next_but'] != '') {
                        next_vid.innerHTML = data['next_but'];
                        }
                        if (data['next_but'] == '') {
                        next_vid.innerHTML = '';
                        }
                        if (type == '0-100') {
                            vid_data.innerHTML = data['video_data'];
                            zerotoh.classList.add("d-none");
                            consume.classList.remove("d-none");
                            consume_back.setAttribute('onclick', "backDetail('0-100')");
                        }
