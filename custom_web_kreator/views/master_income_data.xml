                <!-- Chart.js -->
                <script
                        src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/3.9.1/chart.min.js"></script>
                <!-- Chart Data -->
                <script>
                    const ctx1_labels = JSON.parse(document.getElementById('state_labels').value.trim().replaceAll("'",'"'));
                    const ctx1_data = JSON.parse(document.getElementById('state_data').value.trim().replaceAll("'", '"'));
                    const ctx2_labels = JSON.parse(document.getElementById('course_labels').value.trim().replaceAll("'",'"'));
                    const ctx2_data = JSON.parse(document.getElementById('course_data').value.trim().replaceAll("'", '"'));

                    const ctx1 = document.querySelector("#doughnutChart-one").getContext("2d");
                    const ctx2 = document.querySelector("#doughnutChart-two").getContext("2d");

                    const doughnutChartOne = new Chart(ctx1, {
                    type: "doughnut",
                    data: {
                    labels: ctx1_labels,
                    datasets: [
                    {
                    label: "",
                    data: ctx1_data,
                    backgroundColor: [
                    "rgba(255, 205, 0, 1)",
                    "rgba(0, 0, 0, 1)",
                    "rgba(8, 0, 102, 1)",
                    "rgba(255, 119, 0, 1)",
                    "rgba(255, 245, 198, 1)",
                    ],
                    borderColor: [
                    "rgba(255, 205, 0, 0.7)",
                    "rgba(0, 0, 0, 0.7)",
                    "rgba(8, 0, 102, 0.7)",
                    "rgba(255, 119, 0, 0.7)",
                    "rgba(255, 245, 198, 0.7)",
                    ],
                    borderWidth: 1,
                    },
                    ],
                    },
                    options: {
                    responsive: true,
                    maintainAspectRatio: false,
                    plugins: {
                    legend: {
                    display: false,
                    position: "bottom",
                    labels: {
                    font: {
                    size: 14,
                    },
                    padding: 20,
                    },
                    },
                    title: {
                    display: true,
                    text: "State Wise Earnings",
                    font: {
                    size: 18,
                    },
                    padding: {
                    bottom: 10,
                    },
                    },
                    tooltip: {
                    callbacks: {
                    label: function (context) {
                    let label = context.label || "";
                    let value = context.raw || 0;
                    return `₹ ${value} : ${label}`;
                    },
                    },
                    },
                    },
                    cutout: "65%",
                    animation: {
                    animateScale: true,
                    animateRotate: true,
                    duration: 2000,
                    },
                    },
                    });

                    const doughnutChartTwo = new Chart(ctx2, {
                    type: "doughnut",
                    data: {
                    labels: ctx2_labels,
                    datasets: [
                    {
                    label: "",
                    data: ctx2_data,
                    backgroundColor: [
                    "rgba(255, 205, 0, 1)",
                    "rgba(0, 0, 0, 1)",
                    "rgba(8, 0, 102, 1)",
                    "rgba(255, 119, 0, 1)",
                    "rgba(255, 245, 198, 1)",
                    ],
                    borderColor: [
                    "rgba(255, 205, 0, 0.7)",
                    "rgba(0, 0, 0, 0.7)",
                    "rgba(8, 0, 102, 0.7)",
                    "rgba(255, 119, 0, 0.7)",
                    "rgba(255, 245, 198, 0.7)",
                    ],
                    borderWidth: 1,
                    },
                    ],
                    },
                    options: {
                    responsive: true,
                    maintainAspectRatio: false,
                    plugins: {
                    legend: {
                    display: false,
                    position: "bottom",
                    labels: {
                    font: {
                    size: 14,
                    },
                    padding: 20,
                    },
                    },
                    title: {
                    display: true,
                    text: "Course Wise Earnings",
                    font: {
                    size: 18,
                    },
