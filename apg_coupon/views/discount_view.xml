<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_discount_discount_form" model="ir.ui.view">
        <field name="name">discount.discount.form</field>
        <field name="model">discount.discount</field>
        <field name="arch" type="xml">
            <form string="Discount">
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="view_discount_discount_tree" model="ir.ui.view">
        <field name="name">discount.discount.tree</field>
        <field name="model">discount.discount</field>
        <field name="arch" type="xml">
            <list string="Discount">
                <field name="name"/>
            </list>
        </field>
    </record>
    <record id="view_discount_discount_search" model="ir.ui.view">
        <field name="name">discount.discount.search</field>
        <field name="model">discount.discount</field>
        <field name="arch" type="xml">
            <search string="Discount">
                <field name="name"/>
            </search>
        </field>
    </record>
    <record id="action_discount_discount_record" model="ir.actions.act_window">
        <field name="name">Discount</field>
        <field name="res_model">discount.discount</field>
        <field name="view_mode">list,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Create a Data
            </p>
        </field>
    </record>
</odoo>