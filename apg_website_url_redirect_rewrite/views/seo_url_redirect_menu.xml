<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <!-- Action -->
     <record id="action_seo_url_redirect" model="ir.actions.act_window">
        <field name="name">SEO URL Redirect</field>
        <field name="res_model">blog.post</field>  <!-- Using blog.post instead of new model -->
        <field name="view_mode">list,form</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
        <field name="help" type="html">
            <p>Manage SEO URL redirects for blog posts.</p>
        </field>
    </record>

    <!-- Menu Item -->
    <menuitem id="menu_seo_url_redirect"
              name="SEO URL Redirect"
              parent="website.menu_website_global_configuration"
              action="action_seo_url_redirect"
              sequence="60" />
</odoo>
